#include <iostream>
using namespace std;

int main() {
    int number, sum = 0;

    cout << "Enter integers (0 to stop): ";
    while (true) {
        cin >> number;
        if (number == 0)
            break;
        if (number > 0)
            sum += number;
    }

    cout << "Sum of positive integers: " << sum << endl;
    return 0;
}
